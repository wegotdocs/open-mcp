          }
        }
      }
    },
    "emails": {
      "description": "The contact emails",
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "label",
          "value"
        ],
        "properties": {
          "label": {
            "description": "The email label",
            "type": "string"
          },
          "value": {
            "description": "The email value",
            "type": "string"
          },
          "primary": {
            "description": "True if email is primary",
