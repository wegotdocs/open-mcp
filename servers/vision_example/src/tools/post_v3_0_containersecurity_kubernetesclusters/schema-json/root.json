{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the cluster.",
      "example": "example_cluster",
      "maxLength": 64,
      "pattern": "^[a-zA-Z0-9._]+$"
    },
    "groupId": {
      "type": "string",
      "description": "The ID of the group associated with the cluster. \nTo get IDs of the groups within the user's management scope, use the Kubernetes cluster groups API to list these IDs.",
      "example": "60e53669-c8ef-4d0f-a8ff-3dbbb098d8ff",
      "maxLength": 36,
      "format": "uuid"
    },
    "description": {
      "type": "string",
      "description": "The description of the cluster",
      "example": "My cluster description",
      "maxLength": 128
    },
    "policyId": {
      "type": "string",
      "description": "The ID of the policy associated with the cluster. ",
      "example": "60e53669-c8ef-4d0f-a8ff-3dbbb098d8ff",
      "maxLength": 128
    },
    "resourceId": {
      "type": "string",
      "description": "The ID of the cluster of a different cloud provider",
      "example": "arn:aws:eks:us-east-1:123456789012:cluster/cluster-1",
      "maxLength": 160
    }
  },
  "required": [
    "name",
    "groupId"
  ]
}