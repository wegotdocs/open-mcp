{
  "type": "object",
  "properties": {
    "id": {
      "description": "Time Entry ID",
      "type": "string",
      "format": "uuid"
    },
    "projectId": {
      "type": "string",
      "format": "uuid"
    },
    "tagIds": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "comment": {
      "type": "string",
      "format": "text"
    },
    "timeEntryIn": {
      "properties": {
        "date": {
          "type": "string",
          "format": "Y-m-dTH:i:sP",
          "example": "2020-01-01T10:00:00+01:00"
        },
        "coordinates": {
          "properties": {
            "latitude": {
              "type": "number",
              "format": "float"
            },
            "longitude": {
              "type": "number",
              "format": "float"
            }
          }
        }
      }
    },
    "timeEntryOut": {
      "properties": {
        "date": {
          "type": "string",
          "format": "Y-m-dTH:i:sP",
          "example": "2020-01-01T10:00:00+01:00"
        },
        "coordinates": {
          "properties": {
            "latitude": {
              "type": "number",
              "format": "float"
            },
            "longitude": {
              "type": "number",
              "format": "float"
            }
          }
        }
      }
    }
  },
  "required": [
    "id",
    "timeEntryIn"
  ]
}