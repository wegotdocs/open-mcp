{
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "description": "Name of the Project",
      "examples": [
        "Project 'Save the World'"
      ],
      "minLength": 1,
      "title": "Name",
      "type": "string"
    },
    "description": {
      "anyOf": [
        {
          "minLength": 1,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Description of the Project (markdown field - converted to text)",
      "examples": [
        "The world is threatened by a bad guy, save it!"
      ],
      "title": "Description"
    },
    "client": {
      "description": "Name of the client associated to the Project (created if not existing)",
      "examples": [
        "MI6"
      ],
      "minLength": 1,
      "title": "Client",
      "type": "string"
    },
    "category": {
      "anyOf": [
        {
          "minLength": 1,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Name of the category associated to the Project (created if not existing)",
      "examples": [
        "Top Secret"
      ],
      "title": "Category"
    },
    "status": {
      "anyOf": [
        {
          "minLength": 1,
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Name of the status associated to the Project (created if not existing)",
      "examples": [
        "In Progress"
      ],
      "title": "Status"
    },
    "business_units": {
      "description": "List of names of the business units associated to the Project (created if not existing)",
      "examples": [
        [
          "MI6 - Level 8",
          "MI6 - Level 5"
        ]
      ],
      "items": {
        "minLength": 1,
        "type": "string"
      },
      "title": "Business Units",
      "type": "array"
    },
    "estimated_start_date": {
      "anyOf": [
        {
          "format": "date",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Estimated start date of the Project",
      "examples": [
        "2021-01-01"
      ],
      "title": "Estimated Start Date"
    },
    "estimated_end_date": {
      "anyOf": [
        {
          "format": "date",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Estimated end date of the Project",
      "examples": [
        "2021-01-04"
      ],
      "title": "Estimated End Date"
    },
    "archived": {
      "default": false,
      "description": "Whether the Project is archived or not",
      "examples": [
        false
      ],
      "title": "Archived",
      "type": "boolean"
    },
    "locked": {
      "default": false,
      "description": "Whether the Project is locked (modification and staffing disabled on the app) or not",
      "examples": [
        false
      ],
      "title": "Locked",
      "type": "boolean"
    },
    "billing_method": {
      "default": null,
      "description": "Billing method of the Project",
      "enum": [
        "fixed_price",
        "time_and_material",
        null
      ],
      "examples": [
        null
      ],
      "title": "Billing Method",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "target_margin_rate": {
      "anyOf": [
        {
          "type": "number"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Target margin rate on the Project",
      "examples": [
        0.2
      ],
      "title": "Target Margin Rate"
    },
    "man_days_sold": {
      "description": "List of man days sold associated to the Project",
      "items": {
        "properties": {
          "position": {
            "description": "Name of the position sold",
            "enum": [
              "Agents",
              "Special Agents"
            ],
            "examples": [
              "Special Agents"
            ],
            "title": "Position",
            "type": "string"
          },
          "days": {
            "description": "Number of days sold",
            "examples": [
              10.2
            ],
            "minimum": 0,
            "title": "Days",
            "type": "number"
          }
        },
        "required": [
          "position",
          "days"
        ],
        "title": "ManDaysSold",
        "type": "object"
      },
      "title": "Man Days Sold",
      "type": "array"
    },
    "kanban_column": {
      "default": null,
      "description": "Name of the kanban column the Project will be put in. Column names are the ones from the english version of the app",
      "enum": [
        "Waiting List",
        "In Progress",
        "Done",
        null
      ],
      "examples": [
        null
      ],
      "title": "Kanban Column",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "custom_dropdown_fields": {
      "type": "object",
      "description": "<llm-instruction>This part of the input schema is truncated. If you want to pass the property `custom_dropdown_fields` to the tool, first call the tool `expandSchema` with \"/properties/custom_dropdown_fields\" in the list of pointers. This will return the expanded input schema which you can then use in the tool call. You may have to call `expandSchema` multiple times if the schema is nested.</llm-instruction>\n<property-description>Custom dropdown fields of the Project, (see [Custom Dropdown Fields](#tag/Custom-Fields/Custom-Dropdown-Fields))</property-description>",
      "additionalProperties": true
    },
    "custom_text_fields": {
      "type": "object",
      "description": "<llm-instruction>This part of the input schema is truncated. If you want to pass the property `custom_text_fields` to the tool, first call the tool `expandSchema` with \"/properties/custom_text_fields\" in the list of pointers. This will return the expanded input schema which you can then use in the tool call. You may have to call `expandSchema` multiple times if the schema is nested.</llm-instruction>\n<property-description>Custom text fields of the Project, (see [Custom Text Fields](#tag/Custom-Fields/Custom-Text-Fields))</property-description>",
      "additionalProperties": true
    },
    "creator": {
      "anyOf": [
        {
          "properties": {
            "napta_id": {
              "anyOf": [
                {
                  "exclusiveMinimum": 0,
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Napta identifier of the User",
              "examples": [
                1
              ],
              "title": "Napta Id"
            },
            "external_id": {
              "anyOf": [
                {
                  "minLength": 1,
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Unique reference of the User (see [External Ids](#tag/External-Ids))",
              "examples": [
                "agent-007"
              ],
              "title": "External Id"
            }
          },
          "title": "UserIdentifier",
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Identifier of the User that owns the Project (at least one identification value must be provided)",
      "examples": [
        {
          "external_id": "agent-007"
        }
      ]
    },
    "co_owners": {
      "description": "List of identifiers of the Users that are co-owners of the Project (at least one identification value must be provided)",
      "examples": [
        [
          {
            "external_id": "agent-001"
          },
          {
            "external_id": "agent-002"
          }
        ]
      ],
      "items": {
        "properties": {
          "napta_id": {
            "anyOf": [
              {
                "exclusiveMinimum": 0,
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Napta identifier of the User",
            "examples": [
              1
            ],
            "title": "Napta Id"
          },
          "external_id": {
            "anyOf": [
              {
                "minLength": 1,
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Unique reference of the User (see [External Ids](#tag/External-Ids))",
            "examples": [
              "agent-007"
            ],
            "title": "External Id"
          }
        },
        "title": "UserIdentifier",
        "type": "object"
      },
      "title": "Co Owners",
      "type": "array"
    },
    "parent_project": {
      "anyOf": [
        {
          "properties": {
            "napta_id": {
              "anyOf": [
                {
                  "exclusiveMinimum": 0,
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Napta identifier of the Project",
              "examples": [
                1
              ],
              "title": "Napta Id"
            },
            "external_id": {
              "anyOf": [
                {
                  "minLength": 1,
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "description": "Unique reference of the Project (see [External Ids](#tag/External-Ids))",
              "examples": [
                "mission-007"
              ],
              "title": "External Id"
            }
          },
          "title": "ProjectIdentifier",
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Identifier of the Project parent (in project hierarchy) of the current project (at least one identification value must be provided)",
      "examples": [
        {
          "external_id": "mission-006"
        }
      ]
    },
    "fees": {
      "description": "List of fees associated to the Project",
      "items": {
        "properties": {
          "amount": {
            "description": "Amount",
            "examples": [
              1000.5
            ],
            "minimum": 0,
            "title": "Amount",
            "type": "number"
          },
          "currency": {
            "default": null,
            "description": "Currency. If `null`, the currency will be set to the default currency",
            "enum": [
              "EUR",
              "USD",
              "GBP",
              null
            ],
            "examples": [
              null
            ],
            "title": "Currency"
          },
          "name": {
            "description": "Name of the fee",
            "examples": [
              "Speed boat"
            ],
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "date": {
            "description": "Date of the fee",
            "examples": [
              "2021-01-01"
            ],
            "format": "date",
            "title": "Date",
            "type": "string"
          }
        },
        "required": [
          "amount",
          "name",
          "date"
        ],
        "title": "FeeRequest",
        "type": "object"
      },
      "title": "Fees",
      "type": "array"
    },
    "sold_budget_info": {
      "anyOf": [
        {
          "properties": {
            "amount": {
              "description": "Amount",
              "examples": [
                1000.5
              ],
              "minimum": 0,
              "title": "Amount",
              "type": "number"
            },
            "currency": {
              "default": null,
              "description": "Currency. If `null`, the currency will be set to the default currency",
              "enum": [
                "EUR",
                "USD",
                "GBP",
                null
              ],
              "examples": [
                null
              ],
              "title": "Currency"
            }
          },
          "required": [
            "amount"
          ],
          "title": "MonetaryFieldRequest",
          "type": "object"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Sold budget on the Project",
      "examples": [
        {
          "amount": 1000.5,
          "currency": "EUR"
        }
      ]
    }
  },
  "required": [
    "id",
    "name",
    "client"
  ]
}