{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Subaccount name",
      "nullable": false,
      "example": "John Doe",
      "deprecated": false
    },
    "email": {
      "type": "string",
      "description": "Subaccount email",
      "nullable": false,
      "example": "john.doe@asaas.com.br",
      "deprecated": false
    },
    "loginEmail": {
      "type": "string",
      "description": "Email for subaccount login, if not provided, the subaccount email will be used",
      "example": "johndoe@asaas.com.br",
      "deprecated": false
    },
    "cpfCnpj": {
      "type": "string",
      "description": "CPF or CNPJ of the subaccount owner",
      "nullable": false,
      "example": "35381637000150",
      "deprecated": false
    },
    "birthDate": {
      "type": "string",
      "description": "Date of birth (only for Individuals)",
      "format": "date",
      "example": "1995-04-12",
      "deprecated": false
    },
    "companyType": {
      "type": "string",
      "description": "Type of company (only when Legal Entity)",
      "example": "MEI",
      "deprecated": false,
      "enum": [
        "MEI",
        "LIMITED",
        "INDIVIDUAL",
        "ASSOCIATION"
      ]
    },
    "phone": {
      "type": "string",
      "description": "Telephone",
      "deprecated": false,
      "example": null
    },
    "mobilePhone": {
      "type": "string",
      "description": "Cellphone",
      "nullable": false,
      "deprecated": false,
      "example": null
    },
    "site": {
      "type": "string",
      "description": "Url reffered to the subaccount",
      "example": "https://www.example.com",
      "deprecated": false
    },
    "incomeValue": {
      "type": "number",
      "description": "Billing/Monthly income",
      "nullable": false,
      "example": 25000,
      "deprecated": false
    },
    "address": {
      "type": "string",
      "description": "Public place",
      "nullable": false,
      "example": "Rua Fernando Orlandi",
      "deprecated": false
    },
    "addressNumber": {
      "type": "string",
      "description": "Address number",
      "nullable": false,
      "example": "544",
      "deprecated": false
    },
    "complement": {
      "type": "string",
      "description": "Address complement",
      "deprecated": false,
      "example": null
    },
    "province": {
      "type": "string",
      "description": "Neighborhood",
      "nullable": false,
      "example": "Jardim Pedra Branca",
      "deprecated": false
    },
    "postalCode": {
      "type": "string",
      "description": "Address zip code",
      "nullable": false,
      "example": "14079-452",
      "deprecated": false
    },
