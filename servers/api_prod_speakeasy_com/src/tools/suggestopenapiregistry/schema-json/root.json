        "method-names",
        "diagnostics-only"
      ]
    },
    "diagnostics": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Message describing the issue"
          },
          "path": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Schema path to the issue"
          },
          "type": {
            "type": "string",
            "description": "Issue type"
          },
          "helpMessage": {
            "type": "string",
            "description": "Help message for how to fix the issue"
          }
        },
        "required": [
          "message",
          "path",
          "type"
        ]
      }
    },
    "x-session-id": {
      "type": "string"
    }
  },
  "required": [
    "namespace_name",
    "revision_reference",
    "oas_summary",
    "suggestion_type",
    "diagnostics",
    "x-session-id"
  ]
