{
  "type": "object",
  "properties": {
    "MessageID": {
      "description": "Message ID",
      "type": "string",
      "pattern": "^[A-Za-z0-9._]{4,23}-[A-Za-z0-9._]{4,14}(-[A-Za-z0-9._]{4,10})?(-[A-Za-z0-9._]{4,10})?$"
    },
    "to": {
      "type": "string",
      "description": "Chat ID or phone number where the message will be forwarded",
      "pattern": "^[\\d-]{10,31}(@[\\w\\.]{1,})?$"
    },
    "force": {
      "type": "boolean",
      "description": "Force forward message"
    }
  },
  "required": [
    "MessageID",
    "to"
  ]
}