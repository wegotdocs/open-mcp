{
  "type": "object",
  "properties": {
    "ContactID": {
      "description": "Contact ID",
      "type": "string",
      "pattern": "^([\\d]{7,15})?$"
    },
    "count": {
      "description": "Count of objects to return",
      "type": "number",
      "minimum": 1,
      "maximum": 500,
      "default": 100
    },
    "offset": {
      "description": "Offset of objects to return",
      "type": "number",
      "minimum": 0
    }
  },
  "required": [
    "ContactID"
  ]
}