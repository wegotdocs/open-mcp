{
  "type": "object",
  "properties": {
    "NewsletterID": {
      "description": "Newsletter ID",
      "type": "string",
      "pattern": "^[\\d]{10,18}@newsletter$"
    },
    "ContactID": {
      "description": "Contact ID",
      "type": "string",
      "pattern": "^([\\d]{7,15})?$"
    }
  },
  "required": [
    "NewsletterID",
    "ContactID"
  ]
}