{
  "type": "object",
  "properties": {
    "MessageID": {
      "description": "Message ID",
      "type": "string",
      "pattern": "^[A-Za-z0-9._]{4,23}-[A-Za-z0-9._]{4,14}(-[A-Za-z0-9._]{4,10})?(-[A-Za-z0-9._]{4,10})?$"
    },
    "contacts": {
      "type": "array",
      "description": "List of contacts to send the story to",
      "items": {
        "type": "string",
        "description": "Contact ID",
        "pattern": "^([\\d]{7,15})?$"
      }
    },
    "exclude_contacts": {
      "type": "array",
      "description": "List of contacts to exclude",
      "items": {
        "type": "string",
        "description": "Contact ID",
        "pattern": "^([\\d]{7,15})?$"
      }
    }
  },
  "required": [
    "MessageID"
  ]
}