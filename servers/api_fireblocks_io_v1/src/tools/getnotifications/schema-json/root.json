{
  "type": "object",
  "properties": {
    "webhookId": {
      "format": "uuid",
      "example": "44fcead0-7053-4831-a53a-df7fb90d440f",
      "type": "string"
    },
    "order": {
      "description": "ASC / DESC ordering (default DESC)",
      "default": "DESC",
      "example": "ASC",
      "enum": [
        "ASC",
        "DESC"
      ],
      "type": "string"
    },
    "pageCursor": {
      "description": "Cursor of the required page",
      "type": "string"
    },
    "pageSize": {
      "description": "Maximum number of items in the page",
      "minimum": 1,
      "maximum": 100,
      "default": 100,
      "example": 10,
      "type": "number"
    },
    "createdStartDate": {
      "description": "sort by start date",
      "example": "2024-09-24T09:14:38.356Z",
      "type": "string"
    },
    "createdEndDate": {
      "description": "sort by end date",
      "example": "2024-09-24T09:14:38.356Z",
      "type": "string"
    },
    "statuses": {
