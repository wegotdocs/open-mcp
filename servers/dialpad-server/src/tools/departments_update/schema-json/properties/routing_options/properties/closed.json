    "operator_routing": {
      "description": "The routing strategy that should be used when dialing operators.",
      "enum": [
        "fixedorder",
        "longestidle",
        "mostskilled",
        "random",
        "roundrobin",
        "simultaneous"
      ],
      "nullable": true,
      "type": "string"
    },
    "try_dial_operators": {
      "description": "Whether operators should be dialed on inbound calls.",
      "nullable": true,
      "type": "boolean"
    }
  },
  "required": [
    "action",
    "try_dial_operators"
  ],
  "title": "Group routing options for open or closed states."
}