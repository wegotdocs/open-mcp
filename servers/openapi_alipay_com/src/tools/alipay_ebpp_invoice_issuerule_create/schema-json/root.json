    "outer_source_id": {
      "description": "填写该字段可用于创建幂等，防止重复创建",
      "example": "0b26c12b16950007742372557ee431",
      "type": "string"
    },
    "quota_type": {
      "description": "额度类型，枚举：COUPON（点券）、CAP（余额）",
