{
  "type": "object",
  "properties": {
    "start_time": {
      "description": "账务流水创建时间的起始范围。只能查询一年内的记录",
      "example": "2019-01-01 00:00:00",
      "type": "string"
    },
    "end_time": {
      "description": "账务流水创建时间的结束范围。与起始时间间隔不超过31天。查询结果为起始时间至结束时间的左闭右开区间",
      "example": "2019-01-02 00:00:00",
      "type": "string"
    },
    "alipay_order_no": {
      "description": "支付宝订单号，通过支付宝订单号精确查询相关的流水明细，商户订单号与支付宝订单号互斥",
      "example": "20190101***",
      "type": "string"
    },
    "merchant_order_no": {
      "description": "商户订单号，通过商户订单号精确查询相关的流水明细，商户订单号与支付宝订单号互斥",
      "example": "TX***",
      "type": "string"
    },
    "page_no": {
      "description": "分页号，从1开始",
      "example": "1",
      "type": "string"
    },
    "page_size": {
      "description": "分页大小1000-2000，默认2000",
      "example": "2000",
      "type": "string"
    },
    "trans_code": {
      "description": "账务的类型代码，特殊场景下使用",
      "example": "101101,301101",
      "type": "string"
    },
    "agreement_no": {
      "description": "协议授权码，特殊场景下使用",
      "example": "20215606000635888888",
      "type": "string"
    },
    "agreement_product_code": {
      "description": "协议产品码。特殊场景下使用",
      "example": "FUND_SIGN_WITHHOLDING",
      "type": "string"
    },
    "bill_user_id": {
      "description": "指定用户做账单查询",
      "example": "2088123456789012",
      "type": "string"
    }
  },
  "required": []
}