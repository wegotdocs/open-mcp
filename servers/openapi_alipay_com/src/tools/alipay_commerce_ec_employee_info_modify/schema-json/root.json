      "type": "string"
    },
    "employee_id": {
      "description": "员工id",
      "example": "22887387848484",
      "type": "string"
    },
    "employee_mobile": {
      "description": "员工手机号。如果不传值，则不更新手机号。",
      "example": "158****4364",
      "type": "string"
    },
    "employee_name": {
      "description": "员工姓名",
      "example": "张三",
      "type": "string"
    },
    "employee_no": {
      "description": "员工工号。如果不传值，则不更新工号。",
      "example": "D83030",
      "type": "string"
    },
    "enterprise_id": {
      "description": "企业id",
      "example": "2088474883838",
