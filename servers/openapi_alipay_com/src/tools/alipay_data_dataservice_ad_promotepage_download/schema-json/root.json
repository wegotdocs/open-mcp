{
  "type": "object",
  "properties": {
    "start_date": {
      "description": "留资开始日期，格式：yyyy-mm-dd 或 yyyy-mm-dd hh:mm:ss;不能早于30天前",
      "example": "2020-12-12 00:00:00",
      "type": "string"
    },
    "end_date": {
      "description": "留资结束日期，格式：yyyy-mm-dd 或 yyyy-mm-dd hh:mm:ss;不能晚于当天",
      "example": "2020-12-13 23:59:59",
      "type": "string"
    },
    "page_no": {
      "description": "分页参数之页数，从1开始",
      "example": 1,
      "type": "integer"
    },
    "page_size": {
      "description": "分页参数之每页大小，最大1000",
      "example": 100,
      "type": "integer"
    },
    "biz_token": {
      "description": "代理商访问灯火平台的token",
      "example": "HYjofejnnlp89730202bcklHH",
      "type": "string"
    },
    "principal_tag": {
      "description": "商家标志",
      "example": "yfvbv133498",
      "type": "string"
    },
    "promote_page_id": {
      "description": "推广页id",
      "example": 79900056,
      "type": "integer"
    }
  },
  "required": []
}