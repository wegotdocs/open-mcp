{
  "type": "object",
  "properties": {
    "attribute": {
      "description": "商品属性列表",
      "items": {
        "properties": {
          "name": {
            "description": "商品属性名",
            "example": "材质",
            "type": "string"
          },
          "value": {
            "description": "商品属性值",
            "example": "[\"纯棉\",\"条轮\"]",
